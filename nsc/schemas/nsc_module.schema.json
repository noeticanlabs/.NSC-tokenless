{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "NSC Module",
  "type": "object",
  "required": ["module_id","registry_id","types","nodes","entrypoints","order"],
  "properties": {
    "module_id": { "type": "string", "minLength": 1 },
    "registry_id": { "type": "string", "minLength": 1 },
    "types": {
      "type": "object",
      "additionalProperties": { "$ref": "nsc_type.schema.json" }
    },
    "nodes": {
      "type": "array",
      "items": { "$ref": "nsc_node.schema.json" },
      "minItems": 1
    },
    "entrypoints": {
      "type": "object",
      "additionalProperties": { "type": "integer", "minimum": 1 }
    },
    "order": {
      "type": "object",
      "required": ["eval_seq_id"],
      "properties": {
        "eval_seq_id": { "type": "integer", "minimum": 1 }
      },
      "additionalProperties": true
    },
    "meta": { "type": "object", "additionalProperties": true },
    "module_digest": { "$ref": "nsc_module_digest.schema.json" }
  },
  "additionalProperties": false
}
